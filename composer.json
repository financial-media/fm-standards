{
  "name": "treehouselabs/standards",
  "description": "Development standards by and for TreeHouse employees",
  "license": "MIT",
  "authors": [
    {
      "name": "TreeHouse development",
      "email": "dev@treehouse.nl"
    }
  ],
  "config": {
    "bin-dir": "bin",
    "sort-packages": true
  },
  "require": {
    "php": ">=7.1",
    "friendsofphp/php-cs-fixer": "^2.7"
  },
  "require-dev": {
    "phpunit/phpunit": "^6.4"
  },
  "autoload": {
    "psr-4": {
      "TreeHouse\\CS\\": "src/TreeHouse/CS/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Test\\TreeHouse\\CS\\": "test/TreeHouse/CS/"
    }
  }
}
